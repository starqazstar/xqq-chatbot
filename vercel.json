{
    "version": 2,
    "name": "chatbot_openai_api",
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "installCommand": "pip install -r requirements.txt"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}
